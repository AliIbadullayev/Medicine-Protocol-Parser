        "http://java.sun.com/dtd/web-app_2_3.dtd" >


<web-app>
    <display-name>cargo-deploy</display-name>
    <servlet>
        <servlet-name>ClientServlet</servlet-name>
        <servlet-class>org.testorg.client.servlet.ClientServlet</servlet-class>
    </servlet>
    
